{
    "name": "deg540/docker-php-lista-compra",
    "description": "Docker PHP kata TDD Lista compra",
    "type": "project",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.10"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Deg540\\DockerPHPListaCompra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deg540\\DockerPHPListaCompra\\Test\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "scripts": {
        "test": [
            "phpunit --configuration phpunit.dist"
        ]
    }
}
